{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    
    "rules": {
        "class-methods-use-this": ["off"]
    },
    "globals": {
        "cy": "readonly",
        "Cypress": "readonly",
        "before": "readonly",
        "expect": "readonly"
    }
}
